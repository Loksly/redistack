{
	"entity": "Post",
	"schema": {
		"Id": {"key": true, "type": "Number"},
		"PostTypeId": {"key": true, "type": "Number"},
		"ParentId": {"key": true, "type": "Number"},
		"Tags": {"key": true, "type": "String", "splitBy": "><", "remove": ["<", ">"]},
		"AcceptedAnswerId": "Number",
		"AnswerCount": "Number",
		"Body": "String",
		"ClosedDate": "Date",
		"CommentCount": "Number",
		"CommunityOwnedDate": "Date",
		"CreationDate": "Date",
		"FavoriteCount": "Number",
		"LastActivityDate": "String",
		"LastEditDate": "Date",
		"LastEditorDisplayName": "String",
		"LastEditorUserId": "Number",
		"OwnerDisplayName": "String",
		"OwnerUserId": "Number",
		"Score": "Number",
		"Title": "String",
		"ViewCount": "Number"
	}
}